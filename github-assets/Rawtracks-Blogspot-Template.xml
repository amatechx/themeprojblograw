                                                                    <div id='qodef-m-album-1' class='qodef-m-album jp-jplayer' data-album-id='1'>
                                                                        <audio id='jp_audio_player' preload='metadata'>
                                                                            <source src='https://www.soundjay.com/misc/sounds/bell-ringing-05.wav' type='audio/mpeg'/>
                                                                        </audio>
                                                                    </div>
                                                                    <div id='qodef-m-player-1' class='qodef-m-player' style='background-image: url(https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/images/rbt-placeholder.jpg)'>
                                                                        <div class='qodef-m-player-waveform-progress'>
                                                                            <div class='qodef-m-track-progress jp-gui jp-interface'>
                                                                                <div class='jp-progress'>
                                                                                    <div class='jp-seek-bar'>
                                                                                        <div class='jp-play-bar'></div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class='qodef-m-player-inner'>
                                                                            <div class='qodef-m-player-left'>
                                                                                <div class='qodef-m-album-image'>
                                                                                    <img alt='Album Cover' src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/images/rbt-placeholder.jpg'/>
                                                                                </div>
                                                                                <div class='qodef-m-player-track-info'>
                                                                                    <h4 class='qodef-m-player-track-title jp-title'>Demo Track</h4>
                                                                                    <div class='qodef-m-artist'>
                                                                                        <a class='qodef-m-artist-link' href='#'>Demo Artist</a>
                                                                                    </div>
                                                                                    <div class='qodef-m-info'>
                                                                                        <div class='qodef--info-tags'>
                                                                                            <a href='#' rel='tag'>demo</a>
                                                                                        </div>
                                                                                        <div class='qodef-m-subgenre'>
                                                                                            <a href='#' rel='tag'>electronic</a>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <div class='qodef-m-player-middle'>
                                                                                <div class='qodef-m-player-controls jp-gui jp-interface'>
                                                                                    <ul class='jp-controls'>
                                                                                        <li><a class='jp-previous'>Previous</a></li>
                                                                                        <li><a class='jp-play'>Play</a></li>
                                                                                        <li><a class='jp-next'>Next</a></li>
                                                                                    </ul>
                                                                                </div>
                                                                                <div class='qodef-m-track-time jp-gui jp-interface'>
                                                                                    <span class='qodef-m-track-time-current jp-current-time'>00:00</span>/<span class='qodef-m-track-time-duration jp-duration'>00:00</span>
                                                                                </div>
                                                                            </div>
                                                                            <div class='qodef-m-player-right'>
                                                                                <div class='qodef-m-volume-control jp-gui jp-interface'>
                                                                                    <div class='jp-volume-controls'>
                                                                                        <div class='jp-volume-bar'>
                                                                                            <div class='jp-volume-bar-value'></div>
                                                                                        </div>
                                                                                        <a class='jp-mute'>Mute</a>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class='qodef-m-playlist jp-type-playlist'>
                                                                            <div class='jp-playlist'>
                                                                                <ul class='tracks-list'>
                                                                                    <li class='jp-playlist-current'>
                                                                                        <div>
                                                                                            <a class='jp-playlist-item jp-playlist-current'>Demo Track</a>
                                                                                        </div>
                                                                                    </li>
                                                                                </ul>
                                                                            </div>
                                                                        </div>
                                                                        <div class='qodef-m-spinner'>
