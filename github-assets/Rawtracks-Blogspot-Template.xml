@import url('https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/css/responsive.css');
    ]]>
    </b:skin>

    <!-- Fonts -->
    <link href='https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;900&amp;display=swap' rel='stylesheet' type='text/css'/>
    <link href='https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@300;400;500;600;700&amp;display=swap' rel='stylesheet' type='text/css'/>

    <!-- External Scripts -->
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/vendors/js/jquery.min.js'></script>
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/vendors/js/gsap.min.js'></script>
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/vendors/js/ScrollTrigger.min.js'></script>
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/vendors/js/swiper.min.js'></script>
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/js/audio-player.js'></script>
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/js/animations.js'></script>
    <script src='https://raw.githubusercontent.com/amatechx/themeprojblograw/master/github-assets/js/blogspot-navigation.js'></script>
</head>

<body>
