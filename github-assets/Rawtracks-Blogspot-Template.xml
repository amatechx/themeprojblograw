                                                <div class='elementor-column elementor-col-100 elementor-top-column'>
                                                    <div class='elementor-widget-wrap elementor-element-populated'>
                                                        <div class='elementor-element elementor-widget-rawtracks_core_album_list'>
                                                            <div class='elementor-widget-container'>
                                                                <div class='qodef-shortcode qodef-m qodef-album-list qodef-item-layout--info-below qodef-filter-position--left qodef-filter-width--17 qodef--distort-animation-list qodef-grid qodef-layout--columns qodef-gutter--medium qodef-col-num--4 qodef-item-layout--info-below qodef-filter--on qodef--no-bottom-space qodef-pagination--off'>
                                                                    <div class='qodef-m-filter'>
                                                                        <div class='qodef-m-filter-items'>
                                                                            <a class='qodef-m-filter-item qodef--active' data-filter='*'>
                                                                                <span class='qodef-m-filter-item-name'>Show All</span>
                                                                            </a>
                                                                            <a class='qodef-m-filter-item' data-filter='.beat'>
                                                                                <span class='qodef-m-filter-item-name'>Beat</span>
                                                                            </a>
                                                                            <a class='qodef-m-filter-item' data-filter='.minimal'>
                                                                                <span class='qodef-m-filter-item-name'>Minimal</span>
                                                                            </a>
                                                                            <a class='qodef-m-filter-item' data-filter='.trap'>
                                                                                <span class='qodef-m-filter-item-name'>Trap</span>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                    <div class='qodef-grid-inner clear'>
                                                                        <!-- Album items will be populated dynamically -->
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </section>

                                        <!-- Blog Posts Section (Dynamic Content) -->
                                        <div class='blog-posts-section'>
                                            <b:section class='blog-posts' id='blog-posts' showaddelement='yes'>
                                                <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog' version='2' visible='true'>
                                                    <b:widget-settings>
                                                        <b:widget-setting name='showDateHeader'>false</b:widget-setting>
                                                        <b:widget-setting name='style.textcolor'>#ffffff</b:widget-setting>
                                                        <b:widget-setting name='style.titlecolor'>#ffffff</b:widget-setting>
                                                        <b:widget-setting name='style.linkcolor'>#ffffff</b:widget-setting>
                                                        <b:widget-setting name='style.bgcolor'>transparent</b:widget-setting>
                                                        <b:widget-setting name='showShareButtons'>false</b:widget-setting>
                                                        <b:widget-setting name='showCommentLink'>false</b:widget-setting>
                                                        <b:widget-setting name='showAuthor'>false</b:widget-setting>
                                                        <b:widget-setting name='showDate'>true</b:widget-setting>
                                                        <b:widget-setting name='showLabels'>true</b:widget-setting>
                                                    </b:widget-settings>
                                                    <b:includable id='main'>
                                                        <b:if cond='data:blog.pageType == "index"'>
                                                            <div class='blog-posts-grid'>
                                                                <b:loop values='data:posts' var='post'>
                                                                    <div class='blog-post-item'>
                                                                        <b:if cond='data:post.thumbnail'>
                                                                            <div class='post-thumbnail'>
                                                                                <img expr:src='data:post.thumbnail.url' expr:alt='data:post.title'/>
                                                                            </div>
                                                                        </b:if>
                                                                        <h3 class='post-title'>
                                                                            <a expr:href='data:post.url'><data:post.title/></a>
                                                                        </h3>
                                                                        <div class='post-meta'>
                                                                            <span class='post-date'><data:post.dateHeader/></span>
                                                                            <span class='post-labels'>
                                                                                <b:loop values='data:post.labels' var='label'>
                                                                                    <a expr:href='data:label.url' rel='tag'><data:label.name/></a>
                                                                                </b:loop>
                                                                            </span>
                                                                        </div>
                                                                        <div class='post-snippet'>
                                                                            <data:post.snippet/>
                                                                        </div>
                                                                    </div>
                                                                </b:loop>
                                                            </div>
                                                            <b:include name='nextprev'/>
                                                        <b:else/>
                                                            <b:include name='post'/>
                                                        </b:if>
                                                    </b:includable>
                                                </b:widget>
                                            </b:section>
                                        </div>

                                    </div>
                                </div>
                            </main>
                        </div>
                    </div>

